source! /Users/yuutas4/.vimperatorrc.local

" vim: set ft=vimperator:

set complete=sfbtSh
set hintchars=jfnvurhgytbkdmcielsxoe
set gui=addons,nobookmarks,nomenu,nonavigation,tas
set titlestring="Firefox"
set hintchars=ifjklasdhguonmerwc

map h <C-p>
map l <C-n>
noremap j 3<C-e>
noremap k 3<C-y>

noremap [Prefix] i
nmap i [Prefix]

noremap <C-l> <C-n>
noremap <C-h> <C-p>
noremap <C-g> :open javascript:document.querySelector(".is-selected-tweet").parentNode.parentNode.parentNode.parentNode.querySelector(".js-action-header").click()<CR>
noremap <C-t> t
noremap gh <Nop>
noremap F ;o

echo .vimperatorrc sourced
